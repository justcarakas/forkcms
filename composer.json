{
    "name": "forkcms/forkcms",
    "description": "Fork is an open source CMS that will rock your world.",
    "keywords": [
        "cms",
        "fork",
        "fork cms",
        "content management system",
        "symfony",
        "blog",
        "php"
    ],
    "homepage": "http://www.fork-cms.com/",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "behat/transliterator": "~1.0",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/orm": "^2.8",
        "google/apiclient": "^2.9",
        "google/recaptcha": "~1.1",
        "jeroendesloovere/geolocation-php-api": "^2.1",
        "league/flysystem": "~1.0",
        "league/flysystem-aws-s3-v3": "^1.0.13",
        "league/flysystem-cached-adapter": "^1.0.6",
        "liip/imagine-bundle": "^2.5",
        "matthiasmullie/minify": "~1.3",
        "matthiasmullie/scrapbook": "^1.3",
        "pimple/pimple": "^3.2",
        "ramsey/uuid": "^4.1",
        "ramsey/uuid-doctrine": "^1.2",
        "simple-bus/doctrine-orm-bridge": "^4.0",
        "spoon/library": "^3.0",
        "swiftmailer/swiftmailer": "^6.0",
        "symfony/monolog-bundle": "^3.7",
        "symfony/swiftmailer-bundle": "^3.0",
        "tijsverkoyen/akismet": "1.1.*",
        "friendsofsymfony/rest-bundle": "^3.0",
        "jms/serializer-bundle": "^3.9",
        "codex-team/editor.js": "^2.0",
        "beberlei/doctrineextensions": "^1.2",
        "symfony/webpack-encore-bundle": "^1.7",
        "tijsverkoyen/css-to-inline-styles": "^2.2",
        "phpoffice/phpspreadsheet": "^1.12",
        "composer/package-versions-deprecated": "^1.11",
        "symfony/flex": "^1.12",
        "sensio/framework-extra-bundle": "^6.1",
        "symfony/asset": "^5.2",
        "symfony/mailer": "^5.2",
        "symfony/form": "^5.2",
        "symfony/security-bundle": "^5.2",
        "symfony/translation": "^5.2",
        "symfony/validator": "^5.2",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "symfony/proxy-manager-bridge": "^5.2",
        "symfony/twig-bundle": "^5.2",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "symfony/debug-bundle": "^5.2",
        "symfony/yaml": "^5.2",
        "doctrine/annotations": "^1.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "symfony/property-access": "^5.2",
        "symfony/property-info": "^5.2",
        "symfony/serializer": "^5.2"
    },
    "require-dev": {
        "jdorn/sql-formatter": "1.2.17",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/phpunit-bridge": "*",
        "phpstan/phpstan": "^0.12.52",
        "phpstan/phpstan-symfony": "^0.12.7",
        "phpstan/phpstan-doctrine": "^0.12.22",
        "symfony/var-dumper": "^5.2",
        "symfony/dotenv": "^5.2",
        "symfony/maker-bundle": "^1.31",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "symfony/stopwatch": "^5.2",
        "symfony/web-profiler-bundle": "^5.2"
    },
    "config": {
        "bin-dir": "bin"
    },
    "support": {
        "forum": "https://fork-cms.herokuapp.com",
        "issues": "https://github.com/forkcms/forkcms/issues"
    },
    "scripts": {
        "test": "./bin/simple-phpunit",
        "post-install-cmd": [],
        "post-update-cmd": []
    },
    "autoload": {
        "psr-4": {
            "ForkCMS\\": "src"
        },
        "exclude-from-classmap": [
            "**/Tests/",
            "**/tests/",
            "**/Test/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ForkCMS\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    }
}
